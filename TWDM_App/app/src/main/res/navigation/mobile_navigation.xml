<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.twdm_app.ui.networkinterfaces.InterfacesFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.twdm_app.ui.networkscan.WiFiScanFragment"
        android:label="@string/title_wifi"
        tools:layout="@layout/fragment_wifiscan" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.twdm_app.ui.communication.CommunicationFragment"
        android:label="@string/title_com"
        tools:layout="@layout/fragment_communication" />
</navigation>